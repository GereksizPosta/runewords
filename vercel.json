{
	"version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
	],
	
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
		},
		{
			"src": "/styles/",
			"dest": "./styles/"
		},
		{
			"src": "/assets/",
			"dest": "./assets/"
		}

	],
	"headers": [
		{
		"source": "/devs/:dev_name",
		"headers" : [
		  {
			"key" : "Access-Control-Allow-Origin",
			"value" : "/*"
		  },
		  {
			"key" : "Access-Control-Allow-Headers",
			"value" : "Origin, X-Requested-With, Content-Type, Accept"
		  },
		  {
			"key" : "Access-Control-Allow-Credentials",
			"value" : "true"
		  }
		]
	}
	]
}