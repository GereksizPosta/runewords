{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
		},
		{
			"src": "/styles/",
			"dest": "./styles/"
		},
		{
			"src": "/assets/",
			"dest": "./assets/"
		}

    ]
}